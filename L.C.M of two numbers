def calculate_lcm(x,y):
    if x>y:
        greater=x
    else:
        greater=y
    while(True):
        if((greater%x==0)and(greater%y==0)):
            lcm=greater
            break
        greater+=1
    return lcm
x=int(input("first number"))  
y=int(input("second number"))
print("the L.C.M OF",x,"and",y,calculate_lcm(x,y))
